SliderText.Parent = PlayFrame

local SliderBar = Instance.new("Frame")
SliderBar.Size = UDim2.new(0.9, 0, 0, 6)
SliderBar.Position = UDim2.new(0.045, 0, 0.78)
SliderBar.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
